" filetype plugin indent on
syntax on
colorscheme habamax

set nobackup nowritebackup
set noswapfile

set number
set relativenumber

set tabstop=4
set shiftwidth=4
set softtabstop=4
set expandtab
" set autoindent
set smarttab

set mouse=a
set cursorline

set scrolloff=5

set ignorecase
